---
const { class: className, ...rest } = Astro.props;

const paths = [
  {
    label: 'Home',
    href: '/',
  },
  {
    label: 'About',
    href: '/about/',
  },
  {
    label: 'Posts',
    href: '/posts/page/',
  },
  {
    label: 'Tags',
    href: '/tags/',
  },
];
---

<ul class={className} {...rest}>
  {
    paths.map((path) => (
      <li {...rest}>
        <a href={path.href} {...rest}>
          {path.label}
        </a>
      </li>
    ))
  }
</ul>
